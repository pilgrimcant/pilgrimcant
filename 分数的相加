//Adds two fractions

#include <stdio.h>

int main()
{
  int num1, denom1, num2, denom2, result_num, result_denom;
	
  printf("输入第一个分数：");
  scanf("%d/%d",&num1,&denom1);
	
  printf("输入第二个分数：");
  scanf("%d/%d",&num2,&denom2),
	
  result_num = num1 * denom2 + num2* denom1;
  result_denom = denom1 *denom2;
	
  printf("分数之和为%d/%d\n",result_num,result_denom);
	
	
